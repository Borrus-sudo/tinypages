{
  "name": "tinypages",
  "private": "true",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "tsup-node",
    "dev": "tsup-node --watch src",
    "dev-build": "tsup-node --outDir ./demo/out"
  },
  "main": "out/index.js",
  "module": "out/index.mjs",
  "types": "out/index.d.ts",
  "files": [
    "out",
    "bin"
  ],
  "bin": {
    "tinypages": "./bin/tinypage.js"
  },
  "exports": {
    ".": {
      "require": "./out/cli.js",
      "import": "./out/cli.mjs",
      "types": "./out/cli.d.ts"
    },
    "./entry-server": {
      "require": "./out/entry-server.js",
      "import": "./out/entry-server.mjs"
    },
    "./package.json": {
      "require": "./package.json",
      "import": "./package.json"
    }
  },
  "dependencies": {
    "@tinypages/compiler": "^1.0.0",
    "cac": "^6.7.12",
    "express": "^4.17.2",
    "ohmyfetch": "^0.4.14",
    "picocolors": "^1.0.0",
    "preact": "^10.5.15",
    "preact-render-to-string": "^5.1.19",
    "vite": "^2.7.2"
  },
  "devDependencies": {}
}
