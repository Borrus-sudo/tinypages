{
  "name": "tinypages",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "tsup-node",
    "dev": "tsup-node --watch src"
  },
  "module": "./out/node/cli.mjs",
  "files": [
    "out",
    "bin",
    "types"
  ],
  "bin": {
    "tinypages": "./bin/tinypage.js"
  },
  "exports": {
    ".": {
      "import": "./out/node/cli.mjs",
      "types": "./out/cli.d.ts"
    },
    "./entry-server": {
      "import": "./out/node/entry-server.mjs"
    },
    "./client": {
      "import": "./out/client/client.mjs"
    },
    "./types": {
      "require": "./types/index.d.ts"
    }
  },
  "dependencies": {
    "@astropub/webapi": "^0.10.14",
    "@prefresh/vite": "^2.2.8",
    "@tinypages/compiler": "^1.0.0",
    "@unocss/vite": "^0.31.2",
    "cac": "^6.7.12",
    "consola": "^2.15.3",
    "defu": "^6.0.0",
    "express": "^4.17.3",
    "million": "^1.8.0",
    "node-icons": "^1.0.1",
    "ohash": "^0.1.0",
    "ohmyfetch": "^0.4.14",
    "ora": "^6.1.0",
    "preact": "10.7.1",
    "preact-helmet": "^4.0.0-alpha-3",
    "preact-iso": "^2.3.0",
    "preact-render-to-string": "5.1.21",
    "radix3": "^0.1.0",
    "unconfig": "^0.3.2",
    "vite": "^2.9.1",
    "vite-plugin-image-presets": "^0.3.1",
    "vite-plugin-inspect": "^0.4.3"
  },
  "devDependencies": {
    "@types/connect": "^3.4.35"
  }
}
