{
  "name": "tinypages",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "tsup-node",
    "dev": "tsup-node --watch src"
  },
  "main": "out/index.js",
  "module": "out/index.mjs",
  "types": "types/dynamicTypes.ts",
  "files": [
    "out",
    "bin",
    "types"
  ],
  "bin": {
    "tinypages": "./bin/tinypage.js"
  },
  "exports": {
    ".": {
      "require": "./out/node/cli.js",
      "import": "./out/node/cli.mjs",
      "types": "./out/cli.d.ts"
    },
    "./entry-server": {
      "require": "./out/node/entry-server.js",
      "import": "./out/node/entry-server.mjs"
    },
    "./package.json": {
      "require": "./package.json",
      "import": "./package.json"
    },
    "./client": {
      "require": "./out/client/client.js",
      "import": "./out/client/client.mjs"
    },
    "./server": {
      "require": "./out/node/server.js",
      "import": "./out/node/server.mjs"
    },
    "./types": {
      "require": "./types/dynamicTypes.ts",
      "import": "./types/dynamicTypes.ts",
      "types": "./types/dynamicTypes.ts"
    }
  },
  "dependencies": {
    "@tinypages/compiler": "^1.0.0",
    "@unocss/vite": "^0.22.7",
    "cac": "^6.7.12",
    "express": "^4.17.2",
    "micromorph": "^0.0.3",
    "million": "^1.4.3",
    "node-html-parser": "^5.2.0",
    "node-icons": "^0.1.4",
    "node-object-hash": "^2.3.10",
    "ohmyfetch": "^0.4.14",
    "ora": "5.4.1",
    "picocolors": "^1.0.0",
    "preact": "latest",
    "preact-helmet": "^4.0.0-alpha-3",
    "preact-render-to-string": "latest",
    "radix3": "^0.1.0",
    "unconfig": "^0.2.2",
    "vite": "^2.8.4"
  },
  "devDependencies": {
    "@types/connect": "^3.4.35",
    "@types/node": "^17.0.21"
  }
}
