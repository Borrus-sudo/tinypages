{
  "name": "@tinypages/compiler",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "tsup-node",
    "dev": "tsup-node --watch lib",
    "start": "tsup-node demo/index.ts --outDir ./demo/out --no-dts&&node demo/out/index.js"
  },
  "main": "out/index.js",
  "module": "out/index.mjs",
  "types": "out/index.d.ts",
  "files": [
    "out",
    "types"
  ],
  "exports": {
    ".": {
      "require": "./out/index.js",
      "import": "./out/index.mjs",
      "types": "./out/index.d.ts"
    },
    "./utils": {
      "require": "./out/wrapObject.js",
      "import": "./out/wrapObject.mjs",
      "types": "./out/wrapObject.d.ts"
    }
  },
  "devDependencies": {
    "@iconify/json": "^2.1.27",
    "@types/marked": "^4.0.1",
    "@types/node-emoji": "^1.8.1",
    "@unocss/preset-wind": "^0.22.6"
  },
  "dependencies": {
    "headless-mermaid": "^1.2.1",
    "katex": "^0.15.1",
    "marked": "^4.0.12",
    "node-emoji": "^1.11.0",
    "node-html-parser": "^5.2.0",
    "node-icons": "^1.0.0",
    "param-case": "^3.0.4",
    "shiki": "^0.10.0"
  },
  "optionalDependencies": {
    "@unocss/core": "^0.23.0"
  }
}
