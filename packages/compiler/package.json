{
  "name": "@tinypages/compiler",
  "private": "true",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "tsup-node",
    "dev": "tsup-node --watch lib",
    "start": "tsup-node demo/index.ts --outDir ./demo/out --no-dts&&node demo/out/index.js"
  },
  "main": "out/index.js",
  "module": "out/index.mjs",
  "types": "out/index.d.ts",
  "files": [
    "out"
  ],
  "exports": {
    ".": {
      "require": "./out/index.js",
      "import": "./out/index.mjs",
      "types": "./out/index.d.ts"
    }
  },
  "devDependencies": {
    "@iconify/json": "^1.1.458",
    "@types/katex": "^0.11.1",
    "@types/marked": "^4.0.1",
    "@types/node-emoji": "^1.8.1"
  },
  "dependencies": {
    "html-tags": "^3.1.0",
    "katex": "^0.15.1",
    "marked": "^4.0.10",
    "node-emoji": "^1.11.0",
    "node-html-parser": "^5.2.0",
    "node-icons": "^0.1.4",
    "param-case": "^3.0.4",
    "parse-key-value": "^1.0.0",
    "proxy-hookified": "^1.1.4",
    "shiki": "^0.10.0"
  },
  "optionalDependencies": {
    "headless-mermaid": "^1.2.1",
    "windicss": "^3.4.2"
  }
}
